    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "knex": "^3.0.1",
    "pg": "^8.11.3",
    "morgan": "^1.10.0",
    "express-rate-limit": "^7.1.5",
    "nodemailer": "^6.9.7",
